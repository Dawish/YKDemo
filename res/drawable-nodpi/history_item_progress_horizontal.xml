<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0px" />

            <gradient
                android:angle="270.0"
                android:centerColor="#ff292929"
                android:centerY="0.75"
                android:endColor="#ff292929"
                android:startColor="#ff292929" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0px" />

                <gradient
                    android:angle="270.0"
                    android:centerColor="#ff17c0ff"
                    android:centerY="0.75"
                    android:endColor="#ff17c0ff"
                    android:startColor="#ff17c0ff" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0px" />

                <gradient
                    android:angle="270.0"
                    android:centerColor="#ff2fc7ff"
                    android:centerY="0.75"
                    android:endColor="#ff00baff"
                    android:startColor="#ff83e0ff" />
            </shape>
        </clip>
    </item>

</layer-list>